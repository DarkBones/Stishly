<div class="modal edit_transaction_form <%= 'fade' if Rails.env.test? == false %>" id="edit_transaction_form" tabindex="-1" role="dialog" aria-labelledby="edit_transaction_form_label" aria-hidden="true">
  <div class="modal-dialog modal-xl modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="edit_transaction_form_label"><%= t('modal_titles.edit_transaction') %></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>

      <div id="edit_transaction_form_fields">

        <%#= render partial: "transactions/edit_transaction" if Rails.env.test? %>

      </div>

    </div>
  </div>
</div>

<% if Rails.env == 'test' %>
  <script type="text/javascript">
     disableEnter();
  </script>
<% end %>