<tr next_occurrence="<%= schedule.time_num %>">
  <td>
    <%= schedule.name %>
  </td>
  <td>
    <%= User.format_date(schedule.run_date) if schedule.run_date %>
  </td>
  <td>
    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#schedule_transactions_form" id="schedule-transactions-button" onclick="//scheduleTransactionsForm(<%= schedule.id %>);">
      Transactions
    </button>
  </td>
</tr>