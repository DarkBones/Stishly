<tr next_occurrence="<%= schedule.time_num %>">
  <td>
    <%= schedule.name %>
  </td>
  <td>
    <%= User.format_date(schedule.run_date) if schedule.run_date %>
  </td>
  <td>
    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#schedule_transactions_form" id="schedule-transactions-button_<%= schedule.id %>" onclick="scheduleTransactionsForm(<%= schedule.id %>);">
      Transactions
    </button>
  </td>
  <td>
    <button type="button" class="btn btn-primary" data-togle="model" data-target="#schedule_new_transaction_form" id="schedule-new-transaction-button_<%= schedule.id %>" onclick="scheduleNewTransactionsForm(<%= schedule.id %>);">
      New Transaction
    </button>
  </td>
</tr>