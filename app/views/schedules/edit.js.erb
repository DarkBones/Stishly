<% if @schedule.invalid? %>
  triggerNotice("<%= format_form_errors("schedule", @schedule.errors) %>", true);
  $('#editscheduleform').effect( "shake", { direction: "left", times: 4, distance: 6} );
<% else %>
	$("[data-dismiss=modal]").trigger({ type: "click" });
	<%= render partial: "/schedules/update_schedule_table", :locals => {:schedule => @schedule} %>
<% end %>

